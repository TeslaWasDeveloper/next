{
  "name": "next",
  "version": "1.0.0",
  "description": "A boilerplate template for building modern websites with Next.js, designed for fast development and easy customization.",
  "author": "Rucilos",
  "license": "MIT",
  "private": true,
  "keywords": [
    "Next.js",
    "Radix UI",
    "Shadcn UI",
    "Tailwind CSS",
    "TypeScript",
    "React"
  ],
  "engines": {
    "node": ">=20",
    "yarn": ">=4"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf .next",
    "format": "prettier --write .",
    "postinstall": "husky",
    "prepare": "husky",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,json,yml,md,mdx,graphql}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "shadcn": "^3.8.4",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vanilla-cookieconsent": "3.1.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.10",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.12.0"
}
